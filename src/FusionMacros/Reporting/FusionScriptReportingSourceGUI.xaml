<UserControl x:Class="RxdSolutions.FusionScript.Reporting.FusionScriptReportingSourceGUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RxdSolutions.FusionScript.Reporting"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="White">
    <Grid>
    
        <ComboBox ItemsSource="{Binding Scripts}"
                  SelectedValue="{Binding ScriptId}"
                  Height="35"
                  Width="500"
                  DisplayMemberPath="Name"
                  SelectedValuePath="Id" />
    
    </Grid>
</UserControl>
